{
  "name": "LTVPlus",
  "version": "1.0",
  "description": "RESTful based LTVPlus sample API package",
  "require": {
    "silex/silex": "~2.0",
    "monolog/monolog": "~1.22",
    "symfony/console": "~2.5",
    "league/fractal": "^0.17.0",
    "symfony/twig-bridge": "^3.3",
    "white-october/pagerfanta-bundle": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.3",
    "symfony/browser-kit": "~3.0",
    "squizlabs/php_codesniffer": "^3.1",
    "phpmd/phpmd": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "LTVPlus\\": "src/LTVPlus"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LTVPlus\\Test\\": "test"
    }
  },
  "minimum-stability": "stable"
}
